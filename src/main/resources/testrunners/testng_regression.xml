<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" thread-count="3" parallel ="tests">

	<listeners>
		<listener class-name="com.qa.opencart.listeners.ExtentReportListener" />
		<listener class-name="com.qa.opencart.listeners.TestAllureListener" />
		<listener class-name="com.qa.opencart.listeners.AnnotationTransformer" />
	</listeners>

	<test name="Login Page Test">
	<parameter name="browser" value ="chrome" />
	<parameter name="browserversion" value ="111.0" />
    <classes>
      <class name="com.qa.opencart.tests.LoginPageTest"/>
    </classes>
  	</test> 

   <test name="Account Page Test">
    <parameter name="browser" value ="chrome" />
    <parameter name="browserversion" value ="113.0" />
    <classes>
      <class name="com.qa.opencart.tests.AccountsPageTest"/>
    </classes>
 	</test>
  
  <test name="Register User Page Test">
  <parameter name="browser" value ="chrome" />
  <parameter name="browserversion" value ="90.0" />
    <classes>
      <class name="com.qa.opencart.tests.RegisterUserPageTest"/>
    </classes>
  </test>
  
